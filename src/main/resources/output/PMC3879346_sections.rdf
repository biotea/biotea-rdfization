                                        <sio:SIO_000028 rdf:resource="http://bio2rdf.org/pmc_resource:3879346_paragraph_Materials-and-Methods_Chloroplast-Genome-Sequencing_para_1"/>
                                        <sio:SIO_000068 rdf:resource="http://bio2rdf.org/pmc_resource:3879346_section_Materials-and-Methods"/>
                                        <sio:SIO_000008>
