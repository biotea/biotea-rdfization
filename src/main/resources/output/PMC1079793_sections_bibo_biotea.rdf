                                                    <biotea:paragraphList>
                                                      <rdf:Seq rdf:about="http://biotea.ws/listOfParagraphs/pmcdoc_resource/1079793/list11">
                                                        <rdf:li rdf:resource="http://biotea.ws/paragraph/pmcdoc_resource/1079793/Discussion:para_1"/>
                                                        <rdf:li rdf:resource="http://biotea.ws/paragraph/pmcdoc_resource/1079793/Discussion:para_2"/>
                                                        <rdf:li rdf:resource="http://biotea.ws/paragraph/pmcdoc_resource/1079793/Discussion:para_3"/>
