                                          </bibo:Document>
                                        </bibo:cites>
                                        <bibo:cites rdf:resource="http://biotea.ws/reference/pmcdoc_resource/1079793/B17"/>
                                        <bibo:cites rdf:resource="http://biotea.ws/reference/pmcdoc_resource/1079793/B16"/>
